{
  "name": "onetimephoto",
  "version": "0.1.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nodotpics/onetime.git"
  },
  "devDependencies": {
    "@nestjs/cli": "^11.0.16",
    "eslint": "^9.39.2",
    "prettier": "^3.8.1",
    "turbo": "^2.8.3",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": ">=18"
  },
  "homepage": "https://github.com/nodotpics/onetime",
  "license": "MIT",
  "packageManager": "bun@1.2.18",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "turbo run check-types"
  },
  "workspaces": [
    "apps/*",
    "packages/*"
  ]
}
